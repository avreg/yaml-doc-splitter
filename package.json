{
   "name": "@avreg/yaml-doc-splitter",
   "version": "0.0.1",
   "description": "A Node.js transform stream to split multi-doc YAML stream",
   "private": true,
   "engines": {
      "node": "^10.17.0"
   },
   "files": [
      "src/index.ts"
   ],
   "main": "dist/yaml-doc-splitter.js",
   "type": "commonjs",
   "types": "dist/index.d.ts",
   "scripts": {
      "prepare": "husky install",
      "lint": "eslint src test",
      "clean": "rimraf ./dist",
      "build": "yarn clean && webpack --mode production -c webpack.config.cjs",
      "build:dev": "yarn clean && webpack --mode development -c webpack.config.cjs",
      "test": "jest"
   },
   "keywords": [
      "yaml",
      "doc",
      "multidoc",
      "multi-doc",
      "multi",
      "split",
      "stream"
   ],
   "author": "Andrey Nikitin <support@avreg.net>",
   "license": "Apache License 2.0",
   "devDependencies": {
      "@babel/core": "^7.18.5",
      "@babel/preset-env": "^7.18.2",
      "@babel/preset-typescript": "^7.17.12",
      "@commitlint/cli": "^17.0.3",
      "@commitlint/config-conventional": "^17.0.3",
      "@tsconfig/node10": "^1.0.9",
      "@tsconfig/node12": "^1.0.11",
      "@types/jest": "^28.1.3",
      "@types/node": "^17.0.15",
      "@typescript-eslint/eslint-plugin": "^5.18.0",
      "@typescript-eslint/parser": "^5.10.2",
      "babel-jest": "^28.1.1",
      "eslint": "^8.18.0",
      "eslint-config-prettier": "^8.3.0",
      "eslint-config-standard": "^16.0.3",
      "eslint-plugin-import": "^2.25.4",
      "eslint-plugin-jest": "^26.5.3",
      "eslint-plugin-node": "^11.1.0",
      "eslint-plugin-prettier": "^4.0.0",
      "eslint-plugin-promise": "^5.0.0",
      "eslint-webpack-plugin": "^3.1.1",
      "husky": "^8.0.1",
      "jest": "^28.1.1",
      "js-yaml": "^4.1.0",
      "prettier": "^2.5.1",
      "rimraf": "^3.0.2",
      "source-map-loader": "^3.0.1",
      "terser-webpack-plugin": "^5.3.0",
      "ts-loader": "^9.2.6",
      "ts-node": "^10.8.2",
      "typescript": "^4.5.5",
      "webpack": "^5.68.0",
      "webpack-cli": "^4.9.2"
   },
   "husky": {
      "hooks": {
         "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
         "pre-commit": "pretty-quick --staged"
      }
   }
}
